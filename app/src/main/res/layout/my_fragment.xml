                            android:layout_height="@dimen/dp_28"
                            android:layout_gravity="center"
                            android:background="#D8D8D8" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dp_6"
                            android:text="余额充值"
