                            android:textColor="#FF9A9A9A"
                            android:textSize="@dimen/sp_10" />

                    </RelativeLayout>

                    <View
                        android:layout_width="@dimen/dp_1"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/dp_10"
