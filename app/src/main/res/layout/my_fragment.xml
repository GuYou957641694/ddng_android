                        <RelativeLayout
                            android:id="@+id/re3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_10">

                            <ImageView
                                android:id="@+id/iv_id4"
                                android:layout_width="@dimen/dp_28"
                                android:layout_height="@dimen/dp_28"
