                        android:gravity="center"
                        android:padding="@dimen/dp_10">

                        <RelativeLayout
                            android:id="@+id/re5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_10">

                            <ImageView
