<?xml version="1.0" encoding="utf-8"?><android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:background="@android:color/white">    <TextView        android:id="@+id/personal_phone_phone"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:gravity="center"        android:padding="10dp"        android:text="手机拍照"        android:textColor="@color/gray_cc"        android:textSize="@dimen/dp_14"        app:layout_constraintBottom_toTopOf="@id/personal_phone_camera" />    <View        android:layout_width="match_parent"        android:layout_height="1dp"        android:background="#efefef"        app:layout_constraintBottom_toTopOf="@id/personal_phone_camera"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent" />    <TextView        android:id="@+id/personal_phone_camera"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:gravity="center"        android:padding="10dp"        android:text="相册"        android:textColor="@color/gray_cc"        android:textSize="15sp"        app:layout_constraintBottom_toTopOf="@id/personal_phone_finish"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent" />    <View        android:layout_width="match_parent"        android:layout_height="1dp"        android:background="#efefef"        app:layout_constraintBottom_toTopOf="@id/personal_phone_finish"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent" />    <TextView        android:id="@+id/personal_phone_finish"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:gravity="center"        android:padding="10dp"        android:text="取消"        android:textColor="@color/theme_orange"        android:textSize="15sp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent" /></android.support.constraint.ConstraintLayout>